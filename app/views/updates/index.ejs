<% include ../header %>

<h1><%- title %></h1>

<% if (updates.length) { %>
	<ul>
		<% updates.forEach(function (update) { %>
			<li>
				<%= formatDate(update.dateCreated) %>:
				<strong><%= update.title %>.</strong>
				<%= update.description %>
			</li>
		<% }) %>
	</ul>
<% } %>

<% include ../footer %>